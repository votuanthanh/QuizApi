<!DOCTYPE html>
<!--[if IE 9]><html class="lt-ie10" lang="en" > <![endif]-->
<html class="no-js" lang="en" >

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/css/styles.css">
  <title>Result Quiz</title>
</head>

<body>
  <table>
    <thead>
      <tr>
        <th>Full Name</th>
        <th>Email</th>
        <th>Role</th>
        <th>Logic</th>
        <th>Coding</th>
        <th>English</th>
        <th>Total Duration</th>
        <th>Total</th>
      </tr>
    </thead>
    <tbody>
      <% results.forEach(function(result){ %>
        <tr>
          <td data-column="Full Name"><%- result.user.fullName %></td>
          <td data-column="Email"><%- result.user.email %></td>
          <td data-column="Role"><%- result.role %></td>
          <td data-column="Logic"><%- result.exam['1'] %></td>
          <td data-column="Coding"><%- result.exam['2'] %></td>
          <td data-column="English"><%- result.exam['3'] %></td>
          <td data-column="Total Duration"><%- result.duration %></td>
          <td data-column="Total"><%- result.lastResult %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>

</html>